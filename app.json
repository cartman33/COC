{
  "expo": {
    "name": "COC",
    "slug": "coc",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "icon": "./assets/images/footer.png",

    "scheme": "coc",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/footer.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cartman33.coc"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/footer.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/coc.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
      }
    }
  }
}
